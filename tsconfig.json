{
    "compilerOptions": {
      "target": "ESNext",
      "baseUrl": "./src",
      "paths": {
        "@/*": ["*"],
      },
      "module": "NodeNext",
      "moduleResolution": "NodeNext",
      "esModuleInterop": true,
      "strict": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true
    },
    "include": ["./scripts", "./test", "./typechain-types"]
  }
  